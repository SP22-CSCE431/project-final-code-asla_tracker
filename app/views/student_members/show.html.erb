<p id="notice"><%= notice %></p>
<section class="show-box">
<div class="info">
<h1>Viewing Entry <%=@student_member.id%> of the Student Members Table</h1>
<p>
  <strong>Uin:</strong>
  <%= @student_member.uin %>
</p>

<p>
  <strong>First name:</strong>
  <%= @student_member.first_name %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @student_member.last_name %>
</p>

<p>
  <strong>Class year:</strong>
  <%= @student_member.class_year %>
</p>

<p>
  <strong>Student Classification:</strong>
  <%= @student_member.student_classification %>
</p>

<p>
  <strong>Program Level:</strong>
  <%= @student_member.program_level %>
</p>

<p>
  <strong>Join date:</strong>
  <%= @student_member.join_date %>
</p>

<p>
  <strong>Email:</strong>
  <%= @student_member.email %>
</p>

<p>
  <strong>Phone number:</strong>
  <%= @student_member.formatted_phone %>
</p>

<p>
  <strong>Expected graduation date:</strong>
  <%= @student_member.expected_graduation_date %>
</p>

<p>
  <strong>Dues paid:</strong>
  <%= read_bool(@student_member.dues_paid) %>
</p>

<br/>

<p>
  <strong>Social point amount:</strong>
  <%= @student_member.social_point_amount %>
</p>

<p>
  <strong>Meeting point amount:</strong>
  <%= @student_member.meeting_point_amount %>
</p>

<p>
  <strong>Fundraiser point amount:</strong>
  <%= @student_member.fundraiser_point_amount %>
</p>

<p>
  <strong>Informational point amount:</strong>
  <%= @student_member.informational_point_amount %>
</p>

<br/>

<p>
  <strong>Member title:</strong>
  <%= @student_member.member_title %>
</p>



<%= link_to 'All members', student_members_path, class: 'btn btn-primary btn-sm' %> 
<% if session[:isAdmin] %>
<%= link_to 'Edit', edit_student_member_path(@student_member) , class: 'btn btn-primary btn-sm' %> 
<% end %>
<%= link_to 'Back', :back , class: 'btn btn-primary btn-sm' %>
</div>
</section>
